{
  "name": "github-image-resizer",
  "version": "0.0.3",
  "description": "Resizes images pushed to Github and saves them to S3, replacing the original on Github with a lo-res version",
  "main": "app.js",
  "keywords": [
    "odk",
    "github"
  ],
  "scripts": {
    "test": "npm run lint && tape test/*.js | tap-spec",
    "lint": "standard",
    "coverage": "istanbul cover tape -- test/**/*.test.js",
    "start": "node server.js"
  },
  "license": "MIT",
  "dependencies": {
    "async": "^2.1.4",
    "aws-sdk": "^2.625.0",
    "body-parser": "^1.10.0",
    "debug": "^2.6.0",
    "defaults": "^1.0.3",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "github-webhook-middleware": "~0.0.2",
    "hubfs.js": "~1.0.0",
    "knox": "^0.9.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "octokat": "^0.10.0",
    "parse-github-payload": "^0.1.1",
    "queue": "^4.2.1",
    "request": "^2.79.0",
    "run-parallel-limit": "^1.0.3",
    "run-series": "^1.1.4",
    "sharp": "^0.24.1"
  },
  "devDependencies": {
    "body-parser": "^1.16.0",
    "buffer-equal": "1.0.0",
    "dotenv": "^4.0.0",
    "image-size": "^0.8.3",
    "image-size-stream": "^1.1.0",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.4",
    "knox": "^0.9.2",
    "ngrok": "^3.2.7",
    "standard": "^14.3.1",
    "supertest": "^4.0.2",
    "tap-prettify": "0.0.2",
    "tap-spec": "^5.0.0",
    "tape": "^4.6.3",
    "tmp": "^0.1.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/digidem/github-image-resizer.git"
  },
  "author": "Gregor MacLennan",
  "bugs": {
    "url": "https://github.com/digidem/github-image-resizer/issues"
  },
  "homepage": "https://github.com/digidem/github-image-resizer",
