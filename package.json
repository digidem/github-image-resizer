{
  "name": "github-image-resizer",
  "version": "0.0.2",
  "description": "Resizes images pushed to Github and saves them to S3, replacing the original on Github with a lo-res version",
  "main": "app.js",
  "keywords": [
    "odk",
    "github"
  ],
  "scripts": {
    "test": "jshint --exclude=node_modules/* **/*.js && tape test/*.js | tap-spec",
    "coverage": "istanbul cover tape -- test/**/*.test.js",
    "start": "node server.js"
  },
  "license": "MIT",
  "dependencies": {
    "async": "^0.9.0",
    "blitline-resizer": "~0.2.3",
    "body-parser": "^1.10.0",
    "debug": "^2.1.0",
    "defaults": "^1.0.0",
    "express": "^4.10.4",
    "github-webhook-middleware": "~0.0.2",
    "hubfs.js": "~0.0.3",
    "newrelic": "^1.17.1",
    "octokat": "^0.4.7",
    "parse-github-payload": "^0.1.0",
    "request": "^2.49.0",
    "simple_blitline_node": "git+https://github.com/gmaclennan/simple_blitline_node"
  },
  "devDependencies": {
    "body-parser": "^1.12.0",
    "buffer-equal": "0.0.1",
    "dotenv": "^0.5.1",
    "image-size-stream": "^1.0.0",
    "istanbul": "^0.3.6",
    "jshint": "^2.6.3",
    "knox": "^0.9.2",
    "ngrok": "^0.1.99",
    "supertest": "^0.15.0",
    "tap-prettify": "0.0.2",
    "tap-spec": "^2.2.2",
    "tape": "^3.5.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/digidem/github-image-resizer.git"
  },
  "author": "Gregor MacLennan",
  "bugs": {
    "url": "https://github.com/digidem/github-image-resizer/issues"
  },
  "homepage": "https://github.com/digidem/github-image-resizer",
  "jshintConfig": {
    "laxcomma": true,
    "laxbreak": true
  }
}
